{
  "name": "Theo_Task",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "ios13": "npx react-native run-ios --simulator=\"iPhone 13\"",
    "lint": "eslint .",
    "start": "npx react-native start",
    "start:reset": "npx react-native start --reset-cache",
    "test": "jest",
    "install-pods": "yarn pod-install --non-interactive --quiet",
    "postinstall": "patch-package && yarn install-pods && yarn jetify",
    "xcode": "cd ios && open Theo_Task.xcworkspace && cd .."
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@types/i18n-js": "^3.8.4",
    "axios": "^1.4.0",
    "i18n-js": "3.8.0",
    "patch-package": "^8.0.0",
    "pod-install": "^0.1.38",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-native": "0.71.0",
    "react-native-animation-hooks": "^1.0.1",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-svg": "^13.11.0",
    "react-native-toast-message": "^2.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.14.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.20",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "0.73.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "react-native"
  }
